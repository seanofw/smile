	return %Type%_Lt(value, %Type%_Zero) ? SmileUnboxed%Type%_From(%Type%_Zero)
		: %Type%_Gt(value, %Type%_Zero) ? SmileUnboxed%Type%_From(%Type%_One)
		: SmileUnboxed%Type%_From(%Type%_OneHalf);
}

SMILE_EXTERNAL_FUNCTION(Rect)
