	value = %Type%_Abs(value);

	return %Type%_Ge(value, %Type%_One) ? SmileUnboxed%Type%_From(%Type%_Zero)
		: SmileUnboxed%Type%_From(%Type%_Sub(%Type%_One, value));
}

